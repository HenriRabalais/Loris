{
  "compilerOptions": {
    "target": "es5",
    "module": "es2020",
    "baseUrl": ".",
    "paths": {
      "*": ["*", "jsx/*"]
    },
    "sourceMap": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxFactory": "h",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "lib": [
      "es6", "dom", "es2019"
    ],
    "resolveJsonModule": true
  }
}
